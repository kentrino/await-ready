{
  "name": "await-ready",
  "version": "0.0.1",
  "description": "Check if a service is ready",
  "bin": {
    "is-ready": "./dist/main.mjs"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/main.mjs",
  "exports": {
    ".": {
      "import": "./dist/main.mjs"
    }
  },
  "scripts": {
    "build": "tsdown",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "fmt": "oxfmt --write .",
    "fmt:check": "oxfmt --check .",
    "check": "bun run lint && bun run fmt:check",
    "prepublishOnly": "bun run build"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0",
    "tsdown": "^0.20.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
